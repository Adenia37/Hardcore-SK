<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Simple Animal Prosthetics -->

	<ResearchProjectDef>
		<defName>SimpleAnimalProsthetics</defName>
		<label>Simple Animal Prosthetics</label>
		<description>Learn to use and craft simple animal prosthetics</description>
		<baseCost>160</baseCost>
		<techLevel>Industrial</techLevel>
		<prerequisites>
			<li>SK_MedicineI</li>
		</prerequisites>
	</ResearchProjectDef>

	<!-- Animal Bionics -->

	<ResearchProjectDef>
		<defName>AnimalBionics</defName>
		<label>Animal Bionics</label>
		<description>Learn to use and craft animal bionics</description>
		<baseCost>700</baseCost>
		<techLevel>Industrial</techLevel>
		<prerequisites>
			<li>SK_BionicsI</li>
			<li>SimpleAnimalProsthetics</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
		<requiredResearchFacilities>
		  <li>MultiAnalyzer</li>
		</requiredResearchFacilities>
	</ResearchProjectDef>

	<!-- Animal Heal -->

	<ResearchProjectDef>
		<defName>AnimalHeal</defName>
		<label>Animal Healing</label>
		<description>Learn to heal old wounds on animals</description>
		<baseCost>600</baseCost>
		<techLevel>Industrial</techLevel>
		<prerequisites>
			<li>SK_MedicineIII</li>
			<li>SimpleAnimalProsthetics</li>
		</prerequisites>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
	</ResearchProjectDef>


</Defs>